        "agent": "09-QA-Engineer",
        "action": "runFullTestSuite",
        "outputs": [
          "testResults"
        ]
      },
      {
        "id": "s2",
        "name": "Security Audit",
        "agent": "08-Security-Engineer",
        "action": "fullSecurityAudit",
        "outputs": [
          "securityReport"
        ]
      },
      {
        "id": "s3",
        "name": "Update Changelog",
        "agent": "10-Technical-Writer",
        "action": "generateChangelog",
        "outputs": [
          "changelog"
        ]
      },
      {
        "id": "s4",
        "name": "Update Version",
        "agent": "07-DevOps-Engineer",
